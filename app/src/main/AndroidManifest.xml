<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="LockedOrientationActivity"
    package="com.rogues.k12.rouge">
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.rogues.k12.rouge.Main4Activity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"/>
        <activity
            android:name="com.rogues.k12.rouge.Main3Activity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"/>
        <activity
            android:name="com.rogues.k12.rouge.Main2Activity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"/>
        <activity
            android:name="com.rogues.k12.rouge.MainActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data android:name="android.support.VERSION"
            android:value="26.1.0"/>
        <meta-data android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT"/>
    </application>

</manifest>